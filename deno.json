{
  "name": "@dgellow/rfc",
  "version": "0.0.1",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./main.ts"
  },
  "publish": {
    "include": [
      "mod.ts",
      "main.ts",
      "src/**/*.ts",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run --allow-ffi main.ts",
    "install": "deno install -g -A --name rfc main.ts"
  },
  "imports": {
    "@dgellow/weew": "jsr:@dgellow/weew@^0.1.3",
    "@libs/xml": "jsr:@libs/xml@^6.0.2",
    "@db/sqlite": "jsr:@db/sqlite@^0.12",
    "@std/path": "jsr:@std/path@^1",
    "@std/fs": "jsr:@std/fs@^1"
  }
}
